{
    "chainer": {
      "in": ["questions"],
      "out": ["par_text"],
      "pipe": [
        {
          "class_name": "bpr",
          "load_path": "{MODEL_PATH}",
          "query_encoder_file": "query_encoder_ru.pth.tar",
          "bpr_index": "wiki_index_rus.idx",
          "pretrained_model": "DeepPavlov/rubert-base-cased",
          "top_n": 1000,
          "in": ["questions"],
          "out": ["par_ids"]
        },
        {
          "class_name": "wiki_sqlite_vocab",
          "in": ["par_ids"],
          "out": ["par_text"],
          "join_docs": false,
          "shuffle": false,
          "load_path": "{DOWNLOADS_PATH}/bpr/rus/ruwiki_eq100.db"
        }
      ]
    },
    "metadata": {
      "variables": {
        "ROOT_PATH": "~/.deeppavlov",
        "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
        "MODELS_PATH": "{ROOT_PATH}/models",
        "MODEL_PATH": "{MODELS_PATH}/bpr/rus"
      },
    "requirements": [
        "{DEEPPAVLOV_PATH}/requirements/faiss.txt"
    ],
    "download": [
        {
          "url": "http://files.deeppavlov.ai/tmp/bpr/bpr_db_rus.tar.gz",
          "subdir": "{DOWNLOADS_PATH}/bpr/rus"
        },
        {
          "url": "http://files.deeppavlov.ai/tmp/bpr/bpr_encoder_index_rus.tar.gz",
          "subdir": "{MODEL_PATH}"
        }
      ]
    }
  }
  
