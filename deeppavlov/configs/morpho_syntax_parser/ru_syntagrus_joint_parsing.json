{
    "chainer": {
        "in": ["x_words"],
        "pipe": [
            {
                "id": "main",
                "class_name": "joint_tagger_parser",
                "tagger": {"config_path": "{CONFIGS_PATH}/morpho_syntax_parser/morpho_ru_syntagrus_bert.json"},
                "parser": {"config_path": "{CONFIGS_PATH}/morpho_syntax_parser/syntax_ru_syntagrus_bert.json"},
                "to_output_string": true,
                "in": ["x_words"],
                "out": ["y_parsed"]
            }
        ],
        "out": ["y_parsed"]
    },
    "metadata": {
        "variables": {
            "CONFIGS_PATH": "{DEEPPAVLOV_PATH}/configs"
        }
    }
}
