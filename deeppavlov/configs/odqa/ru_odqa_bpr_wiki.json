{
  "chainer": {
    "in": ["question_raw"],
    "out": ["best_answer"],
    "pipe": [
      {
        "config_path": "{CONFIGS_PATH}/doc_retrieval/bpr_ru.json",
        "in": ["question_raw"],
        "out": ["paragraphs"]
      },
      {
        "class_name": "string_multiplier",
        "in": ["question_raw", "paragraphs"],
        "out":["questions"]
      },
      {
        "class_name": "logit_ranker",
        "batch_size": 10,
        "squad_model": {"config_path": "{CONFIGS_PATH}/squad/qa_sberquad_psgcls_bert.json"},
        "sort_noans": true,
        "in": ["paragraphs", "questions"],
        "out": ["best_answer", "best_answer_score"]
      }
    ]
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "CONFIGS_PATH": "{DEEPPAVLOV_PATH}/configs"
    },
    "download": [
    ]
  }
}
